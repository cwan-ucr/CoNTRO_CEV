{
    "episode":{
        "num_train_rollouts": 200,
        "rollout_length": 2048,
        "eval_freq": 500,
        "eval_num_eps": 3,
        "max_ep_steps": 1250,
        "generation_ep_steps": 1000,
        "warmup_ep_steps": 0,
        "test_num_eps": 1,
        "seed": 0
    },
    "agent": {
        "agent_type": "sac",
        "single_agent": false,
        "centralized_train": false,
        "independent_train": false
    },
    "ppo":{
        "gae_tau": 0.85,
        "entropy_weight": 0.2,
        "minibatch_size": 256,
        "optimization_epochs": 5,
        "ppo_ratio_clip": 0.3,
        "discount": 0.99,
        "learning_rate": 1e-4,
        "clip_grads": true,
        "gradient_clip": 1.0,
        "value_loss_coef": 1.0,
        "hidden_layer_size": [32, 32],
        "actor_layer_size": [32, 32],
        "critic_layer_size": [32, 32]
    },
    "sac":{
        "tau": 0.005,
        "batch_size": 32,
        "buffer_length": 200000,
        "grad_clip": 5.0,
        "alpha": 0.12,
        "learning_rate": 3e-4,
        "weight_decay": 1e-4,
        "hidden_layer_size": [128, 128],
        "gama": 0.99
    },
    "rule": {
        "rule_set": "cycle",
        "rule_set_params": {"cycle_length": 120, "NS_mult": 1.0, "EW_mult": 1.0, "phase_end_offset": 100}
    },
    "environment": {
        "shape": [4, 4],
        "rush_hour": false,
        "uniform_generation_probability": 0.08,
        "use_vehicle_controller": true,
        "step_length": 1,
        "gather_vehicle_info": true,
        "actuated_tls": false,
        "phase_mode": "hybrid",
        "rand_CAVPR": true,
        "vehicle_type_distribution": {"ICEV": 0.0, "EV": 0.4, "CAV": 0.0, "CAEV": 0.6}
    },
    "multiagent": {
        "reward_interpolation": 0,
        "state_interpolation": 0
    },
    "parallel":{
        "num_workers": 8
    }
}